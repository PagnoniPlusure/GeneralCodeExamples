    "version": "0.2.0",
    "configurations": [
        {
            "name": "Find Rev",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\bin\\${workspaceFolderBasename}.exe",
            "args": [
                "-u=infodba",
                "-p=infodba",
                "-g=dba",
                "-id=000012",
                "-revId=A"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\bin",
            "environment": [],
            "externalConsole": false
        },
        {
            "name": "Rev not found",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\bin\\${workspaceFolderBasename}.exe",
            "args": [
                "-u=infodba",
                "-p=infodba",
                "-g=dba",
                "-id=0000dd12",
                "-revId=A"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\bin",
            "environment": [],
            "externalConsole": false
        },
        {
            "name": "Attach to process",
            "type": "node",
            "request": "attach",
            "processId": "1712"
        },
        {
            "name": "Prova COM Excel",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\bin\\ProvaComExcel.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\bin",
            "environment": [],
            "externalConsole": false
        }
    ]
}